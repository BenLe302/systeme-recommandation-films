    "# Tests de performance\n",
    "def performance_test(endpoint, num_requests=10):\n",
    "    \"\"\"Test de performance d'un endpoint\"\"\"\n",
    "    response_times = []\n",
    "    success_count = 0\n",
    "    \n",
    "    print(f\"üöÄ Test de performance: {endpoint}\")\n",
    "    print(f\"üìä Nombre de requ√™tes: {num_requests}\")\n",
    "    \n",
    "    for i in range(num_requests):\n",
    "        try:\n",
    "            start_time = time.time()\n",
    "            response = requests.get(f\"{API_BASE_URL}{endpoint}\", timeout=10)\n",
    "            end_time = time.time()\n",
    "            \n",
    "            response_time = end_time - start_time\n",
    "            response_times.append(response_time)\n",
    "            \n",
    "            if response.status_code == 200:\n",
    "                success_count += 1\n",
    "            \n",
    "            print(f\"  Requ√™te {i+1}: {response_time:.3f}s (Status: {response.status_code})\")\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"  Requ√™te {i+1}: Erreur - {e}\")\n",
    "    \n",
    "    if response_times:\n",
    "        stats = {\n",
    "            'mean_time': np.mean(response_times),\n",
    "            'median_time': np.median(response_times),\n",
    "            'min_time': np.min(response_times),\n",
    "            'max_time': np.max(response_times),\n",
    "            'std_time': np.std(response_times),\n",
    "            'success_rate': success_count / num_requests * 100\n",
    "        }\n",
    "        \n",
    "        print(f\"\nüìà STATISTIQUES:\")\n",
    "        print(f\"  Temps moyen: {stats['mean_time']:.3f}s\")\n",
    "        print(f\"  Temps m√©dian: {stats['median_time']:.3f}s\")\n",
    "        print(f\"  Temps min: {stats['min_time']:.3f}s\")\n",
    "        print(f\"  Temps max: {stats['max_time']:.3f}s\")\n",
    "        print(f\"  √âcart-type: {stats['std_time']:.3f}s\")\n",
    "        print(f\"  Taux de succ√®s: {stats['success_rate']:.1f}%\")\n",
    "        \n",
    "        return stats, response_times\n",
    "    \n",
    "    return None, []\n",
    "\n",
    "# Tests de performance\n",
    "if api_available:\n",
    "    print(\"‚ö° === TESTS DE PERFORMANCE ===")\n",
    "    \n",
    "    performance_results = {}\n",
    "    \n",
    "    # Test endpoint de sant√©\n",
    "    stats, times = performance_test('/health', 5)\n",
    "    if stats:\n",
    "        performance_results['health'] = {'stats': stats, 'times': times}\n",
    "    \n",
    "    print(\"\" * 50)\n",
    "    \n",
    "    # Test recommandations utilisateur\n",
    "    stats, times = performance_test('/recommend/user/1?model_type=collaborative&algorithm=svd&num_recommendations=5', 3)\n",
    "    if stats:\n",
    "        performance_results['user_recommendations'] = {'stats': stats, 'times': times}\n",
    "    \n",
    "else:\n",
    "    print(\"‚ö†Ô∏è Tests de performance ignor√©s - API non accessible\")"
   ]
