    "def collect_system_metrics():\n",
    "    \"\"\"Collecte des m√©triques syst√®me\"\"\"\n",
    "    metrics = {\n",
    "        'timestamp': datetime.now().isoformat(),\n",
    "        'api_status': 'unknown',\n",
    "        'response_times': [],\n",
    "        'error_rate': 0,\n",
    "        'recommendations_generated': 0\n",
    "    }\n",
    "    \n",
    "    if not api_available:\n",
    "        metrics['api_status'] = 'down'\n",
    "        return metrics\n",
    "    \n",
    "    # Test de plusieurs requ√™tes\n",
    "    test_endpoints = [\n",
    "        '/health',\n",
    "        '/models',\n",
    "        '/recommend/user/1?model_type=collaborative&algorithm=svd&num_recommendations=5',\n",
    "        '/recommend/movie/1?model_type=content&algorithm=tfidf&num_recommendations=5'\n",
    "    ]\n",
    "    \n",
    "    response_times = []\n",
    "    errors = 0\n",
    "    total_recommendations = 0\n",
    "    \n",
    "    for endpoint in test_endpoints:\n",
    "        try:\n",
    "            start_time = time.time()\n",
    "            response = requests.get(f\"{API_BASE_URL}{endpoint}\", timeout=10)\n",
    "            end_time = time.time()\n",
    "            \n",
    "            response_time = end_time - start_time\n",
    "            response_times.append(response_time)\n",
    "            \n",
    "            if response.status_code != 200:\n",
    "                errors += 1\n",
    "            elif 'recommend' in endpoint:\n",
    "                try:\n",
    "                    data = response.json()\n",
    "                    if isinstance(data, list):\n",
    "                        total_recommendations += len(data)\n",
    "                except:\n",
    "                    pass\n",
    "                    \n",
    "        except Exception:\n",
    "            errors += 1\n",
    "    \n",
    "    metrics.update({\n",
    "        'api_status': 'up',\n",
    "        'response_times': response_times,\n",
    "        'avg_response_time': np.mean(response_times) if response_times else 0,\n",
    "        'error_rate': errors / len(test_endpoints) * 100,\n",
    "        'recommendations_generated': total_recommendations\n",
    "    })\n",
    "    \n",
    "    return metrics\n",
    "\n",
    "print(\"üìä === MONITORING SYST√àME ===")\n",
    "\n",
    "# Collecte des m√©triques\n",
    "system_metrics = collect_system_metrics()\n",
    "\n",
    "print(f\"üïê Timestamp: {system_metrics['timestamp']}\")\n",
    "print(f\"üîå Statut API: {system_metrics['api_status']}\")\n",
    "\n",
    "if system_metrics['api_status'] == 'up':\n",
    "    print(f\"‚ö° Temps de r√©ponse moyen: {system_metrics['avg_response_time']:.3f}s\")\n",
    "    print(f\"‚ùå Taux d'erreur: {system_metrics['error_rate']:.1f}%\")\n",
    "    print(f\"üéØ Recommandations g√©n√©r√©es: {system_metrics['recommendations_generated']}\")\n",
    "    \n",
    "    # Alertes\n",
    "    alerts = []\n",
    "    \n",
    "    if system_metrics['avg_response_time'] > 1.0:\n",
    "        alerts.append(\"‚ö†Ô∏è Temps de r√©ponse √©lev√© (>1s)\")\n",
    "    \n",
    "    if system_metrics['error_rate'] > 10:\n",
    "        alerts.append(\"üö® Taux d'erreur √©lev√© (>10%)\")\n",
    "    \n",
    "    if system_metrics['recommendations_generated'] == 0:\n",
    "        alerts.append(\"‚ö†Ô∏è Aucune recommandation g√©n√©r√©e\")\n",
    "    \n",
    "    if alerts:\n",
    "        print(\"\nüö® ALERTES:\")\n",
    "        for alert in alerts:\n",
    "            print(f\"  {alert}\")\n",
    "    else:\n",
    "        print(\"\n‚úÖ Syst√®me en bonne sant√©\")\n",
    "\n",
    "else:\n",
